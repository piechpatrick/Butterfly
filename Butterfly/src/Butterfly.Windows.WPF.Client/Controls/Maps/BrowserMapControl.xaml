<UserControl x:Class="Butterfly.Windows.WPF.Client.Controls.Maps.BrowserMapControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Butterfly.Windows.WPF.Client.Controls.Maps"
             xmlns:mvvm="http://prismlibrary.com/"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance local:BrowserMapControlViewModel, IsDesignTimeCreatable=True}"
             mvvm:ViewModelLocator.AutoWireViewModel="True"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <WebBrowser Source="https://www.google.com/maps/" ></WebBrowser>
    </Grid>
</UserControl>
